		"preact-router": "^2.6.0",
		"unistore": "^3.0.2"
	},
	"devDependencies": {
		"babel-cli": "^6.26.0",
		"babel-loader": "^7.1.2",
		"babel-plugin-transform-react-jsx": "^6.24.1",
		"babel-preset-env": "^1.6.1",
		"babel-preset-es2015": "^6.24.1",
		"nodemon": "^1.13.0",
